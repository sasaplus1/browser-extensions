{
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@commitlint/config-lerna-scopes": "^18.1.0",
    "@tsconfig/strictest": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^7.32.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "fixpack": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": "20",
    "npm": "10"
  },
  "license": "MIT",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  },
  "private": true,
  "scripts": {
    "fix": "run-p -l fix:*",
    "fix:eslint": "eslint --fix .",
    "fix:fixpack": "npx fixpack package.json extensions/*/package.json",
    "fix:prettier": "prettier --write .",
    "lint": "run-p -l lint:*",
    "lint:eslint": "eslint .",
    "lint:fixpack": "npx fixpack --dryRun package.json extensions/*/package.json",
    "lint:prettier": "prettier --check ."
  },
  "workspaces": [
    "extensions/*"
  ]
}
